    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "9.0.1",
    "@stripe/react-stripe-js": "^1.15.0",
    "@stripe/stripe-js": "^1.44.1",
    "@types/bcrypt": "^3.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/hapi__joi": "^17.1.0",
