{
  "name": "teso-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:dev": "nest start --config webpack.config.js --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register node_modules/typeorm/cli.js",
    "migration:generate": "npm run build && cross-var npm run typeorm -- -d ./data-source.ts migration:generate ./migrations/$npm_config_name",
    "migration:up": "npm run build && npm run typeorm migration:run -- -d ./data-source.ts",
    "migration:down": "npm run build && npm run typeorm -- -d ./data-source.ts migration:revert",
    "migration:create": "cross-var npm run typeorm -- migration:create ./migrations/$npm_config_name"
  },
  "dependencies": {
    "@bull-board/fastify": "5.0.0",
    "@casl/ability": "6.3.2",
    "@fastify/static": "6.5.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "9.0.0",
